{
  "title": {
    "en": "Channel up",
    "nl": "Kanaal omhoog",
	"de": "Kanal hoch"
  },
  "type": "boolean",
  "getable": false,
  "setable": true,
  "$flow": {
    "actions": [
      {
        "id": "channel_up",
        "title": {
          "en": "One channel up",
          "nl": "Eén kanaal omhoog",
		  "de": "Einen Kanal hoch"
        }
      }
    ]
  },
  "uiComponent": "button",
  "$speechExamples": {
    "en": ["Switch My TV to the next channel"],
    "nl": ["Zet Mijn TV op het volgende kanaal"],
	"de": ["Schalte meinen TV zum nächsten Kanal"]
  },
  "$speech": {
    "en": {
      "element": {
        "verb": {
          "type": "regex",
          "value": "(switch|turn)"
        },
        "channel": {
          "type": "string",
          "value": "channel"
        },
        "up": {
          "type": "regex",
          "value": "(next|increase|(turn\\s)?up)"
        }
      },
      "group": {
        "channelUp": {
          "set": "(verb) && channel && up",
          "ordered": false,
          "allowDisconnect": true,
          "capturingGroup": true,
          "devices": {
            "capabilities": ["channel_up"]
          }
        }
      }
    },
    "nl": {
      "element": {
        "verb": {
          "type": "pos",
          "value": {
            "pos": "VERB"
          }
        },
        "channel": {
          "type": "regex",
          "value": "kanaal|zender"
        },
        "up": {
          "type": "regex",
          "value": "(omhoog|volgende|volgend)"
        }
      }
    }
  }
}